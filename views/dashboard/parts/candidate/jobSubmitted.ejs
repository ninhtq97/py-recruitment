      <script>
        $(function () {
          $('input[type="text"]').change(function () {
            this.value = $.trim(this.value);
          });
        });
      </script>

      <script type="text/javascript">
        $(function () {
          $('.delete').click(function (e) {
            e.preventDefault()
            const jobSubmittedId = $(this).data('name');
            $.ajax({
              type: 'POST',
              url: `/candidates/dashboard/job-submitted/delete/${jobSubmittedId}`,
              success: function (data) {
                if (data) {
                  swal("Thành công!", "", "success").then(function () {
                    location.href = "/candidates/dashboard/job-submitted";
                  });
                }
                else {
                  swal({
                    type: 'error',
                    title: '',
                    text: 'Có lỗi xảy ra! Vui lòng thử lại',
                    footer: ''
                  })
                }
              },
              error: (request, status, error) => {
                swal(request.responseJSON.message, "", "error");
              },
            });
          });
        });
      </script>
