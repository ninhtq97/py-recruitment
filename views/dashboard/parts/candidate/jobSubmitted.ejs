                <i class="fal fa-briefcase"></i>
                <br>
                Việc làm mới
              </a>
            </li>
          </ul>
        </header>
        <!-- main area -->
        <div class="main-content">
          <div id="page_content">
            <div class="panel mb20">
              <div class="panel-heading border">
                Danh sách hồ sơ đã nộp
              </div>
              <div class="panel-body">
                <div class="card-body">
                  <p class="card-title"></p>
                  <form action="/candidates/dashboard/job-submitted" method="GET">
                    <div class="form-row">
                      <div class="form-group col-md-3">
                        <input type="text" class="form-control" placeholder="Tên vị trí hoặc công ty..." name="name"
                          value="<%= query.name %>" />
                      </div>
                      <div class="form-group col-md-2">
                        <button type="submit" class="btn btn-primary">Tìm kiếm</button>
                      </div>
                    </div>
                  </form>
                  <table class="table table-hover" id="dataTables-example" width="100%">
                    <thead>
                      <tr>
                        <th>STT</th>
                        <th>Vị trí</th>
                        <th>Công ty</th>
                        <th>Thời hạn nộp</th>
                        <th></th>
                      </tr>
                    </thead>
                    <tbody>
                      <% data.forEach((item, index)=> { %>
                        <tr>
                          <th scope="row">
                            <% if(!query.page || query.page==1) { %>
                              <%= index + 1 %>
                                <%}%>
                                  <% if(query.page && query.page !=1) { %>
                                    <%= (index + 0) + ((query.page * 10) - 9)%>
                                      <%}%>
                          </th>
                          <td>
                            <%= item.jobName %>
                          </td>
                          <td>
                            <%= item.companyName %>
                          </td>
                          <td>
                            <%= moment(item.expirationDate).format('DD/MM/YYYY') %>
                          </td>
                          <td>
                            <a href="" class="btn btn-outline-danger btn-rounded delete" data-name="<%= item.id %>"><i
                                class="fas fa-trash"></i></a>
                          </td>
                        </tr>
                        <% }); %>
                    </tbody>
                  </table>
                  <nav aria-label="Page navigation example">
                    <% if (pages> 1) { %>
                      <ul class="pagination justify-content-center">
                        <% var i=(Number(current)> 10 ? Number(current) - 9 : 1) %>
                          <% if (i !==1) { %>
                            <li class="disabled"><a>...</a></li>
                            <% } %>
                              <% for (; i <=(Number(current) + 9) && i <=pages; i++) { %>
                                <% if (i==current) { %>
                                  <li class="active page-item"><a class="page-link">
                                      <%= i %>
                                    </a></li>
                                  <% } else { %>
                                    <li class="page-item"><a class="page-link"
                                        href="/candidates/dashboard/job-submitted?page=<%= i %>&name=<%= query.name %>">
                                        <%= i %>
                                      </a></li>
                                    <% } %>
                                      <% if (i==Number(current) + 9 && i < pages) { %>
                                        <li class="disabled"><a>...</a></li>
                                        <% } %>
                                          <% } %>
                      </ul>
                      <% } %>
                  </nav>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /main area -->
        <!-- /content panel -->

