               function getOriginOptions() {
                  var categoryId = $('#category').val();
                  var cityId = $('#city').val();
                  var keyword = $('#keyword').val().trim();
                  var options = {};

                  if (keyword) {
                     options.keyword = keyword;
                  }
                  if (categoryId) {
                     options.categories = [categories[parseInt(categoryId)]];
                  }
                  if (cityId) {
                     options.locations = [cities[parseInt(cityId)]];
                  }
                  return options;
               }

               function getAllSearchOptions() {
                  var options = getOriginOptions();
                  var exp = $('#exp-advanced').val();
                  var salary = $('#salary-advanced').val();
                  var type = $('#type-advanced').val();
                  var companyField = $('#company-field-advanced').val();
                  if (exp) {
                     options.exp = exp;
                  }
                  if (salary) {
                     options.salary = salary;
                  }
                  if (type) {
                     options.types = [type];
                  }
                  if (companyField) {
                     options.companyField = companyField;
                  }
                  return options;
               }

               function submitFormSearchJob() {
                  var options = getAllSearchOptions();
                  var url = generateJobSearchUrl(options);
                  document.location.href = url;
               }
               function initInitialAdvanceSearch(params) {
                  if (params.types) {
                     for (var i = 0; i < params.types.length; i++) {
                        $('#type-advanced option[value=' + params.types[i] + ']').prop('selected', true);
                     }
                  }
                  if (params.categoryIds) {
                     for (var i = 1; i < params.categoryIds.length; i++) {
                        $('#exp-advanced option[value=' + params.categoryIds[i] + ']').prop('selected', true);
                     }
                  }
                  if (params.cityIds) {
                     for (var i = 1; i < params.cityIds.length; i++) {
                        $('#salary-advanced option[value=' + params.cityIds[i] + ']').prop('selected', true);
                     }
                  }
                  var exp = $('#exp-advanced').val();
                  var salary = $('#salary-advanced').val();
                  var type = $('#type-advanced').val();
                  var city = $('#city').val();
                  var category = $('#category').val();
                  var companyField = $('#company-field-advanced').val();
                  if (exp || salary || type || companyField) {
                     $('.extend-search').css('display', 'block');
                     $('#btn-hidden-advanced-search').show();
                     $('#btn-show-advanced-search').hide();
                     $('#btn-clear-filter').show();
                  }
                  // $('#exp-advanced, #salary-advanced, #type-advanced').select2();
               }

               $(document).ready(function () {
                  $('.container-scroll').each(function () {
                     var ps = new PerfectScrollbar($(this)[0]);
                  });
                  $("#keyword").autocomplete({
                     source: function (request, response) {
                        response($.grep(Object.keys(suggestionMap), function (value) {
                           var match = suggestionMap[value];
                           var normalizedTerm = normalize(request.term);
                           return (match.indexOf(normalizedTerm) !== -1);
                        }));
                     },
                     select: function (event, ui) {
                        $('#keyword').val(ui.item.label);
                        submitFormSearchJob();
                     },
                     delay: 100,
                     autoFocus: false
                  });
                  $('#frm-search-job').submit(function (e) {
                     e.preventDefault();
                     submitFormSearchJob();
                  });
                  $('#btn-clear-filter').click(function (e) {
                     e.preventDefault();
                     var options = getOriginOptions();
                     var url = generateJobSearchUrl(options);
                     $('#exp-advanced').val('').trigger('change');
                     $('#salary-advanced').val('').trigger('change');
                     $('#type-advanced').val('').trigger('change');
                     document.location.href = url;
                  });
                  $('#btn-show-advanced-search').click(function (e) {
                     $('.extend-search').css('display', 'block');
                     $('#btn-clear-filter').show();
                     $('#btn-hidden-advanced-search').show();
                     $('#btn-show-advanced-search').hide();
                  });
                  $('#btn-hidden-advanced-search').click(function (e) {
                     $('.extend-search').css('display', 'none');
                     $('#btn-clear-filter').hide();
                     $('#btn-hidden-advanced-search').hide();
                     $('#btn-show-advanced-search').show();
                  });
                  initInitialAdvanceSearch(initialParams)
                  $('#category, #city').select2();
