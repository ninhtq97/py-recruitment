            var viewportTop = $(window).scrollTop();
            var viewportBottom = viewportTop + $(window).height();

            return elementBottom > viewportTop + elementHeight && elementTop < viewportBottom - elementHeight;
         };

         updateJobInViewPort = () => {
            updatedJobsInViewPort = [];
            $('.job-ta').each(function (index, el) {
               if ($(el).isInViewport()) {
                  let jobId = $(el).data('job-id');
                  updatedJobsInViewPort.push(jobId);

                  // console.log($(el).data('job-id'));
               } else {
                  // do something else
               }
            });

            const filteredArray = updatedJobsInViewPort.filter(value => jobsInViewPort.includes(value));
            let diff = differenceOf2Arrays(updatedJobsInViewPort, filteredArray);
            let diff2 = differenceOf2Arrays(jobsInViewPort, filteredArray);
            if (diff.length) {
               // console.log('Bắt đầu vào viewport', diff)
               for (jobId of diff) {
                  ta('JobInViewPort', {
                     jb_id: jobId,
                     impr_pos: jobPositionMapping[jobId],
                     ts: new Date().getTime(),
                     p: 'JobDetail',
                  })
               }
            }
            if (diff2.length) {
               for (jobId of diff2) {
                  ta('JobOutViewPort', {
                     jb_id: jobId,
                     impr_pos: jobPositionMapping[jobId],
                     ts: new Date().getTime(),
                     p: 'JobDetail',
                  })
               }
               // console.log('Bắt đầu ra viewport', diff2)
            }
            jobsInViewPort = updatedJobsInViewPort;
            // console.log('================')
         };
         if (onFeatureTrackingTa) {
            $(window).on('resize scroll', function () {
               updateJobInViewPort();
            });
            updateJobInViewPort();
         }
      </script>
