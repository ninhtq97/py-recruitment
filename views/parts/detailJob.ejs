      <script type="text/javascript">
         $(function () {
            $(".jobSubmit").click(function (e) {
               e.preventDefault();

               const jobId = $(this).data('name');
               $.ajax({
                  url: `/jobs/submit/${jobId}`,
                  type: 'POST',
                  success: function (data) {
                     if (data == true) {
                        swal("Thành công!", "", "success");
                     }
                     else {
                        swal(data, "", "error");
                     }
                  },
                  error: (request, status, error) => {
                     swal(request.responseJSON.message, "", "error");
                  },
                  cache: false,
                  contentType: false,
                  processData: false
               });
            });

            $('#btnLogin').click(function (e) {
               e.preventDefault()
               const email = $('input[name=emailLogin]').val();
               const password = $('input[name=passwordLogin]').val();
               const role = '<%= role %>';
               let change = true;
               if (!email || !password) {
                  change = false;
                  swal('Vui lòng nhập đầy đủ', "", "error")
               }
               if (change) {
                  const data = { email: email, password: password, role: role };
                  $.ajax({
                     type: 'POST',
                     data: data,
                     url: '/auth/candidate-login',
                     success: function (data) {
                        if (data == true) {
                           location.href = '/jobs/<%= detail.id %>';
                        }
                        else {
                           swal(data, "", "error");
                        }
                     },
                     error: (request, status, error) => {
                        swal(request.responseJSON.message, "", "error");
                     },
                  });
               }
            });

            $("#registerCandidate").click(function (e) {
               e.preventDefault();

               const formData = new FormData();
               formData.append('email', $('input[name=emailRegister]').val());
               formData.append('password', $('input[name=passwordRegister]').val());
               formData.append('name', $('input[name=nameCandidate]').val());
               formData.append('phone', $('input[name=phoneCandidate]').val());

               let change = true;
               if (
                  !formData.get('email') ||
                  !formData.get('password') ||
                  !formData.get('name') ||
                  !formData.get('phone')
               ) {
                  change = false;
                  swal({
                     type: 'error',
                     title: 'Có lỗi...',
                     text: 'Vui lòng điền đầy đủ thông tin !!!',
                     footer: ''
                  })
               } if (change) {
                  $.ajax({
                     url: '/auth/register/candidate',
                     type: 'POST',
                     data: formData,
                     success: function (data) {
                        if (data == true) {
                           swal("Thành công!", "", "success");
                        }
                        else {
                           swal(data, "", "error");
                        }
                     },
                     error: (request, status, error) => {
                        swal(request.responseJSON.message, "", "error");
                     },
                     cache: false,
                     contentType: false,
                     processData: false
                  });
               }
            });
         });
      </script>

