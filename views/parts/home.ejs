      var checkOniewMixin = {
        data: {
          onScreenTrackJobIds: [],
          prevOnScreenTrackJobIds: [],
        },
        methods: {
          checkOnViewPort: (elem, parent) => {
            let viewMoreHeight = $('.view-more').height()
            let viewportBoxTop = parent.offset().top
            let viewportBoxHeight = parent.height()
            let viewportBoxBottom = viewportBoxTop + viewportBoxHeight - viewMoreHeight
            let $window = jQuery(window)
            let viewportTop = $window.scrollTop()
            let viewportHeight = $window.height()
            let viewportBottom = viewportTop + viewportHeight
            let top = elem.offset().top
            let height = elem.height()
            let bottom = top + height
            return bottom >= viewportTop + height && top <= viewportBottom - height && bottom >= viewportBoxTop + height && top <= viewportBoxBottom - height;
          },
          sendTrackViewJobs: (jobInViewPort, jobOutViewPort, box) => {
            if (jobInViewPort.length > 0) {
              for (job of jobInViewPort) {
                ta('JobInViewPort', {
                  jb_id: job.id,
                  ts: new Date().getTime(),
                  p: taPage,
                  b: box,
                  impr_pos: job.index
                })
              }
            }
            if (jobOutViewPort.length > 0) {
              for (job of jobOutViewPort) {
                ta('JobOutViewPort', {
                  jb_id: job.id,
                  ts: new Date().getTime(),
                  p: taPage,
                  b: box,
                  impr_pos: job.index
                })
              }
            }
          },
          trackJobView(listJobs, boxId, boxName) {
            if (onFeatureTrackingTa) {
              this.onScreenTrackJobIds = []
              let prevOnCheckOnViewPort = false
              listJobs.some((element, index) => {
                let company = {
                  id: '',
                  index: ''
                }
                let checkOnViewPort = this.checkOnViewPort($(boxId).find('div[data-id="' + element.id + '"]'), $(boxId + ' .ctn-list-jobs'))
                if (!checkOnViewPort && prevOnCheckOnViewPort) {
                  return true
                }
                prevOnCheckOnViewPort = checkOnViewPort
                if (checkOnViewPort) {
                  company.id = element.id
                  company.index = index + 1
                  this.onScreenTrackJobIds.push(company)
                }
              });

              let jobOutViewPort = this.prevOnScreenTrackJobIds.filter(el => !this.onScreenTrackJobIds.some(e => el.id === e.id))
              let jobInViewPort = this.onScreenTrackJobIds.filter(el => !this.prevOnScreenTrackJobIds.some(e => el.id === e.id))
              if (jobInViewPort.length > 0 || jobOutViewPort.length > 0) {
                this.sendTrackViewJobs(jobInViewPort, jobOutViewPort, boxName)
              }
              this.prevOnScreenTrackJobIds = this.onScreenTrackJobIds
            }
          },
          jobRendered(jobs, box) {
            if (onFeatureTrackingTa) {
              let jobRenderedIds = [];
              for (job of jobs) {
                jobRenderedIds.push(job.id);
              }
              if (jobRenderedIds.length) {
                ta('jobRendered', {
                  ta: new Date().getTime(),
                  p: taPage,
                  b: box,
                  jb_ids: jobRenderedIds,
                })
              }
            }
          }
        }
      }

      function getOriginOptions() {
        var categoryId = $('#category').val();
        var cityId = $('#city').val();
        var keyword = $('#keyword').val().trim();
        var options = {};

        if (keyword) {
          options.keyword = keyword;
        }
        if (categoryId) {
          options.categories = [categories[parseInt(categoryId)]];
        }
        if (cityId) {
          options.locations = [cities[parseInt(cityId)]];
        }
        return options;
      }

      function getAllSearchOptions() {
        var options = getOriginOptions();
        var exp = $('#exp-advanced').val();
        var salary = $('#salary-advanced').val();
        var type = $('#type-advanced').val();
        var companyField = $('#company-field-advanced').val();
        if (exp) {
          options.exp = exp;
        }
        if (salary) {
          options.salary = salary;
        }
        if (type) {
          options.types = [type];
        }
        if (companyField) {
          options.companyField = companyField;
        }
        return options;
      }

      function submitFormSearchJob() {
        var options = getAllSearchOptions();
        var url = generateJobSearchUrl(options);
        document.location.href = url;
      }
      function initInitialAdvanceSearch(params) {
        if (params.types) {
          for (var i = 0; i < params.types.length; i++) {
            $('#type-advanced option[value=' + params.types[i] + ']').prop('selected', true);
          }
        }
        if (params.categoryIds) {
          for (var i = 1; i < params.categoryIds.length; i++) {
            $('#exp-advanced option[value=' + params.categoryIds[i] + ']').prop('selected', true);
          }
        }
        if (params.cityIds) {
          for (var i = 1; i < params.cityIds.length; i++) {
            $('#salary-advanced option[value=' + params.cityIds[i] + ']').prop('selected', true);
          }
        }
        $('#exp-advanced, #salary-advanced, #type-advanced, #company-field-advanced').select2();
      }
      function chunkJobs(jobs, size) {
        var jobTotal = jobs.length;
        var chunkJobs = [];
        for (var i = 0; i < jobTotal; i += size) {
          chunkJobs.push(jobs.slice(i, i + size));
        }
        return chunkJobs;
      }

