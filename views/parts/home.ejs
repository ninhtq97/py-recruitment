        $("#keyword").autocomplete({
          source: function (request, response) {
            response($.grep(Object.keys(suggestionMap), function (value) {
              var match = suggestionMap[value];
              var normalizedTerm = normalize(request.term);
              return (match.indexOf(normalizedTerm) !== -1);
            }));
          },
          select: function (event, ui) {
            $('#keyword').val(ui.item.label);
            submitFormSearchJob();
          },
          delay: 100,
