      #survey-find-job-modal .modal-content {
        border-radius: 6px;
        margin: 100px auto;
      }

      .box-hotline h4 {
        font-size: 16px;
      }

      @media (max-width: 767px) {
        #box-extend-search {
          margin-top: 30px;
        }
      }

      .label-remote {
        color: #c0392b;
        border: solid 1px;
        border-radius: 5px;
        font-size: 12px;
        padding: 0px 9px;
        font-weight: bold;
        margin-right: 3px;
        display: inline-block;
        line-height: 16px;
      }

      [v-cloak] {
        display: none !important;
      }

      #box-banner-vip-company[v-cloak] {
